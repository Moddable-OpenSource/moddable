{
	"build": {
		"MODULES": "$(MODDABLE)/modules",
		"PIU": "$(MODULES)/piu"
	},
	"creation": {
		"chunk": {
			"initial":1048576,
			"incremental":1048576
		},
		"heap": {
			"initial":65536,
			"incremental":65536
		},
		"stack":2048,
		"keys": {
			"available":16386,
			"name":1993,
			"symbol":127
		},
		"main":"main"
	},
	"data": {
		"*": [
		]
	},
	"modules": {
		"~": [
		],
		"piu/*": [
			"$(PIU)/All/piu*",
			"$(PIU)/PC/piu*",
			"$(PIU)/PC/Code/piu*",
			"$(PIU)/PC/Common/piu*"
		],
		"yaml/*": [
			"$(MODDABLE)/xs/tools/yaml/*"
		],
		"*": [
			"./*",
			"$(MODULES)/base/instrumentation/modInstrumentation"
		]
	},
	"preload": [
		"piu/All",
		"piu/PC",
		"io/serial",
		"timer",
		"scan",
		"esptool"
	],
	"resources": {
		"assets/*": "./assets/*",
		"fonts/FiraMono-*": "$(MODDABLE)/examples/assets/scalablefonts/FiraMono/FiraMono-*",
		"fonts/OpenSans-*": "$(MODDABLE)/examples/assets/scalablefonts/OpenSans/OpenSans-*"
	},
	"platforms":{
		"x-lin": {
			"modules": {
				"io/*": [
					"$(MODDABLE)/modules/io/serial/lin/*"
				],
				"piu/*": [
					"$(PIU)/PC/lin/piu*"
				],
				"*": [
					"$(MODDABLE)/modules/base/timer/timer",
					"$(MODDABLE)/modules/base/timer/lin/*"
				]
			},
			"preload": [
			]
		},
		"x-mac": {
			"modules": {
				"io/*": [
					"$(MODDABLE)/modules/io/serial/mac/*"
				],
				"piu/*": [
					"$(PIU)/PC/mac/piu*"
				],
				"*": [
					"$(MODDABLE)/modules/base/timer/*",
					"$(MODDABLE)/modules/base/timer/mac/*"
				]
			},
			"preload": [
			]
		},
		"x-win": {
			"modules": {
				"io/*": [
					"$(MODDABLE)/modules/io/serial/win/*"
				],
				"piu/*": [
					"$(PIU)/PC/win/piu*"
				],
				"*": [
					"$(MODDABLE)/modules/base/timer/*",
					"$(MODDABLE)/modules/base/timer/win/*"
				]
			},
			"preload": [
			]
		}
	}
}
