{
	"build": {
		"TYPINGS": "$(MODDABLE)/typings"
	},
	"modules": {
		"*": [
			"$(TYPINGS)/*"
		],
		"commodetto/*": [
			"$(TYPINGS)/commodetto/*"
		],
		"dns/*": [
			"$(TYPINGS)/dns/*"
		],
		"mc/*": [
			"$(TYPINGS)/mc/*"
		],
		"pins/*": [
			"$(TYPINGS)/pins/*"
		],
		"piu/*": [
			"$(TYPINGS)/piu/*"
		],
		"embedded:provider/*": [
			"$(TYPINGS)/embedded:provider/*"
		],
		"embedded:io/*": [
			"$(TYPINGS)/embedded:io/*"
		]
	}
}
