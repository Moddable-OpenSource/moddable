{
	"modules": {
		"commodetto/parseBMF": "$(COMMODETTO)/commodettoParseBMF",
		"commodetto/parseBMP": "$(COMMODETTO)/commodettoParseBMP",
		"commodetto/parseRLE": "$(COMMODETTO)/commodettoParseRLE",
		"commodetto/Bitmap": "$(COMMODETTO)/commodettoBitmap",
		"commodetto/Poco": "$(COMMODETTO)/commodettoPoco",
		"commodetto/PocoCore": "$(COMMODETTO)/commodettoPocoCore",
		"commodetto/*": "$(COMMODETTO)/commodettoPocoBlit",
		"commodetto/cfe": "$(COMMODETTO)/cfeBMF"
	},
	"preload": "commodetto/*",
	"platforms": {
		"esp": {
			"modules": {
				"setup/commodetto": "$(BUILD)/devices/esp/setup/commodetto"
			},
			"preload": "setup/commodetto"
		},
		"esp32": {
			"modules": {
				"setup/commodetto": "$(BUILD)/devices/esp32/setup/commodetto"
			},
			"preload": "setup/commodetto"
		},
		"pico": {
			"modules": {
				"setup/commodetto": "$(BUILD)/devices/pico/setup/commodetto"
			},
			"preload": "setup/commodetto"
		},
		"gecko": {
			"modules": {
				"setup/commodetto": "$(BUILD)/devices/gecko/setup/commodetto"
			},
			"preload": "setup/commodetto"
		},
		"qca4020": {
			"modules": {
				"setup/commodetto": "$(BUILD)/devices/gecko/setup/commodetto"
			},
			"preload": "setup/commodetto"
		},
		"nrf52": {
			"modules": {
				"setup/commodetto": "$(BUILD)/devices/nrf52/setup/commodetto"
			},
			"preload": "setup/commodetto"
		}

	}
}
