{
	"include": "$(MODDABLE)/examples/manifest_base.json",
	"modules": {
		"*": [
			"./main"
		],
		"pins/i2c": "$(MODULES)/pins/i2c/i2c",
		"pins/smbus": "$(MODULES)/pins/smbus/smbus",
	},
	"preload": [
		"pins/i2c",
		"pins/smbus",
	],
	"platforms": {
		"esp": {
			"modules": {
				"pins/i2c": "$(MODULES)/pins/i2c/i2c",
				"*": [
					"$(MODULES)/pins/i2c/esp/*",
				],
			},
			"preload": [
				"pins/i2c",
			]
		},
	}
}
