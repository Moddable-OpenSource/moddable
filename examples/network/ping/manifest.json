{
	"include": [
		"$(MODDABLE)/examples/manifest_base.json",
		"$(MODDABLE)/examples/manifest_net.json"
	],
	"modules": {
		"*": [
			"./main"
		]
	},
	"preload": "ping",
	"platforms": {
		"qca4020": {
			"modules": {
				"*": [
					"$(BUILD)/devices/qca4020/modules/ping/*"
				]
			}
		},
		"...": {
			"modules": {
				"*": [
					"$(MODDABLE)/modules/network/ping/ping"
				]
			}
		}	
	}
}
