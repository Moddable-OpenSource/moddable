{
	"include": [
		"$(MODDABLE)/examples/manifest_base.json",
		"$(MODDABLE)/examples/manifest_net.json",
		"$(MODDABLE)/modules/network/ble/manifest.json",
	],
	"modules": {
		"*": [
			"./main"
		]
	},
	"ble":{
		"*": [
			"./bleservices/*",
		],
	},
	"platforms": {
		"esp32": {
			"modules": {
				"*": [
					"$(MODULES)/network/net/*",
					"$(MODULES)/network/net/esp/*",
					"$(MODULES)/network/wifi/*",
					"$(MODULES)/network/wifi/esp32/*",
				],
			},
			"preload": [
				"net",
				"wifi"
			]
		},
	}
}
