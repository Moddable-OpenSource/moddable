{
	"include": "$(MODDABLE)/examples/manifest_base.json",
	"modules": {
		"*": [
			"./main",
			"$(MODULES)/network/wifi/*"
		]
	},
	"preload": [
		"wifi"
	],
	"platforms": {
		"esp": {
			"modules": {
				"*": "$(MODULES)/network/wifi/esp/*"
			}
		},
		"esp32": {
			"modules": {
				"*": "$(MODULES)/network/wifi/esp32/*"
			}
		},
		"qca4020": {
			"modules": {
				"*": "$(MODULES)/network/wifi/qca4020/*"
			}
		},
		"...": {
			"error": "WiFi module unsupported"
		}
	}
}

