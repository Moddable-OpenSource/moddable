{
	"include": [
		"$(MODDABLE)/examples/manifest_base.json",
	],
	"modules": {
		"*": [
			  "./main",
			  "./replcore",
		]
	},
	"preload": [
		"repl",
		"replcore",
	],
	"strip": [],
	"platforms": {
		"esp": {
			"modules": {
				"*": [
					"./esp/repl",
				],
			},
		},
		"esp32": {
			"modules": {
				"*": [
					"./esp/repl",
				],
			},
		},
		"x-cli-mac": {
			"modules": {
				"~": [
					"$(MODULES)/files/resource/*",
				],
				"*": [
					"./mac/repl",
				],
			},
		},
		"mac": {
			"error": "use build target \"x-cli-mac\" instead of \"mac\""
		},
		"...": {
			"warning": "unsupported target"
		}
	}
}
