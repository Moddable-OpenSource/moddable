{
	"modules": {
		"*": [
			"$(MODULES)/base/time/*",
			"$(MODULES)/base/time/esp/*",
			"$(MODULES)/base/timer/*",
			"$(MODULES)/base/timer/mc/*",
			"$(MODULES)/files/preference/modPreference",
			"$(MODULES)/files/preference/esp/modPreference"
		]
	},
	"preload": [
		"time",
		"timer"
	],
	"platforms": {
		"nrf52/*": {
			"include": "$(BUILD)/devices/nrf52/targets/$(SUBPLATFORM)/manifest.json"
		}
	}
}
