{
	"build":{
		"ESP32_SUBCLASS": "esp32s3",
		"USE_USB": "1",
		"SDKCONFIGPATH": "$(MODDABLE)/build/devices/esp32/targets/qtpys3/sdkconfig"
	},
	"include": [
		"$(MODULES)/drivers/button/manifest.json",
		"$(MODULES)/drivers/neopixel/manifest.json"
	],
	"modules": {
		"setup/target": "./setup-target"
	},
	"preload": [
		"setup/target",
		"neopixel"
	],
	"config": {
		"screen": "",
		"led": {
			"power": 38,
			"pin": 39,
			"rainbow": true,
			"brightness": 32
		}
	},
	"defines": {
		"i2c": {
			"sda_pin": 41,
			"scl_pin": 40
		},
		"spi": {
			"miso_pin":37,
			"mosi_pin":35,
			"sck_pin": 36
		}
	}
}
