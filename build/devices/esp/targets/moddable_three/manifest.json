{
    "include": [
        "$(MODULES)/drivers/destm32s/manifest.json",
    ],
	"build": {
		"FLASH_MODE": "qio",
		"FLASH_SPEED": "80"
	},
    "config": {
        "screen": "destm32s",
        "touch": "",
        "format": "Gray256",
    },
    "defines": {
        "destm32s": {
            "width": 122,
            "height": 250,
            "moddable_three": 1,
            "colors": "kePaperBlackWhite",
