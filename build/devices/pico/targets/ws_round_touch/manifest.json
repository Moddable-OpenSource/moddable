{
	"include": [
		"$(MODDABLE)/modules/io/manifest.json",
		"$(MODDABLE)/modules/drivers/gc9a01/manifest.json",
		"$(MODULES)/drivers/led/manifest.json",
		"$(MODULES)/drivers/button/manifest.json",
		"$(MODULES)/pins/pwm/manifest.json",
		"$(MODULES)/drivers/sensors/cst816/manifest.json"
	],
	"modules": {
		"setup/target": "../ws_round/setup-target"
	},
	"preload": "setup/target",
	"config": {
		"screen": "gc9a01",
		"backlight": 25,
		"brightness": 50,
		"touch": "",
		"lcd_rst_pin": 12,
		"lcd_cs_pin": 9
	},
	"defines": {
		"i2c": {
			"sda_pin": 26,
			"scl_pin": 27,
			"port": 1
		},
		"spi": {
			"mosi_pin": 11,
			"sck_pin": 10
		},
		"ili9341": {
			"cs_pin": 9,
			"dc_pin": 8,
			"rst_pin": 13,
			"spi_port": 1,
			"backlight_pin": 25,
			"backlight_on": 1,
			"flipx": true
		},
        "cst816": {
			"flipX": true,
			"flipY": true
		}

	}
}
