{
	"include": [
		"$(MODULES)/drivers/led/manifest.json",
		"$(MODULES)/drivers/neopixel/manifest.json",
		"$(MODULES)/drivers/button/manifest.json"
	],
	"modules": {
		"setup/target": "./setup-target"
	},
	"preload": "setup/target",
	"config": {
		"screen": "",
		"touch": "",
		"led": {
			"pin": 17,
			"power": 16,
			"rainbow": true,
			"brightness": 32
		}
	},
	"defines": {
		"i2c": {
			"sda_pin": 2,
			"scl_pin": 3
		},
		"spi": {
			"miso_pin": 20,
			"mosi_pin": 19,
			"sck_pin": 18,
			"port": 0
		}
	}
}
