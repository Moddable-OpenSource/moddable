{
	"modules": {
		"~": [
			"$(MODULES)/network/net/net"
		],
		"*": [
			"./webthings",
			"$(MODULES)/network/http/*",
			"$(MODULES)/network/mdns/*",
			"$(MODULES)/network/dns/moddnsparser"
		],
		"net": "$(MODULES)/network/net/netmdns",
		"dns": "$(MODULES)/network/dns/dns",
		"dns/parser": "$(MODULES)/network/dns/dnsparser",
		"dns/serializer": "$(MODULES)/network/dns/dnsserializer"
	},
	"preload": [
		"http",
		"mdns",
		"net",
		"dns",
		"dns/parser",
		"dns/serializer",
		"webthings"
	],
	"platforms": {
		"...": {
			"warning": "Web Thing API support deprecated"
		}
	}
}
